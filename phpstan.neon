parameters:
    bootstrapFiles:
        - vendor/autoload.php
    level: 9
    phpVersion: 80200
    paths:
        - src
    treatPhpDocTypesAsCertain: false
    ignoreErrors:
        - '#Property .+::\$id \(int\|null\) is never assigned int#'
        - '#Method App\\Component\\Live\\.+::.+ is unused#'


