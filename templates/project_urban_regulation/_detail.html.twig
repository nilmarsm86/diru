{% component 'Card:Card' with {
    cssClass: 'mb-4',
    extra: 'id="project_urban_regulation_detail_' ~ project_urban_regulation.id,
    headerCssClass: 'd-flex justify-content-between align-items-center',
    title: title,
} %}

    {% block header %}
        {{ include('partials/_card_header_options.html.twig', {
            goTo: 'app_project_urban_regulation_new',
            label: 'Agregar',
            title: 'Agregar regulación urbana',
            entity: project_urban_regulation,
            state: 'detail',
            showButton: (showButton is defined and showButton == true) ? true : false
        }) }}
    {% endblock %}

    {% block body %}
        <div class="table-responsive table mt-2" role="grid" aria-describedby="dataTable_info">
            <table class="table table-hover">
                <tbody>
                <tr>
                    <th>Dato:</th>
                    <td>{{ project_urban_regulation.data }}</td>
                </tr>
                </tbody>
            </table>
        </div>
    {% endblock %}

    {% block footer %}
        {{ include('partials/_card_footer.html.twig', {
            list: {href: 'app_project_urban_regulation_index', label: 'Listado', title:'Volver al listado'},
            edit: {href: 'app_project_urban_regulation_edit', label: 'Editar', title:'Editar regulación urbana del proyecto'},
            delete: {href: 'app_project_urban_regulation_delete', label: 'Eliminar', title:'Eliminar regulación urbana del proyecto'},
            id: project_urban_regulation.id,
            message: 'Está seguro que desea eliminar la regulación urbana del proyecto?',
            state: 'detail',
            showButton: (showButton is defined and showButton == true) ? true : false
        }) }}
    {% endblock %}

{% endcomponent %}


